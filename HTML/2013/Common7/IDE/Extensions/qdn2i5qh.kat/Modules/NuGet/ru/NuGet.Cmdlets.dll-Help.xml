          <command:parameterValue required="true">string</command:parameterValue>
        </command:parameter>
      </command:syntaxItem>
    </command:syntax>

    <!-- Cmdlet parameter section  -->
    <command:parameters>
      <command:parameter required="true" position="1">
        <maml:name>ProjectName</maml:name>
        <maml:description>
          <maml:para>Указывает проект для анализа и добавления переадресации привязок.</maml:para>
        </maml:description>
        <command:parameterValue required="true">
          string
        </command:parameterValue>
      </command:parameter>
    </command:parameters>

    <command:examples>
      <command:example>
        <maml:title>----------  ПРИМЕР 1  ----------</maml:title>
        <dev:code>PM&gt; Add-BindingRedirect MyProjectName</dev:code>
        <dev:remarks>
          <maml:para>
            Описание ------------------------------------------------------------------ Добавляет переадресацию привязок в указанный проект, MyProjectName.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
    </command:examples>

