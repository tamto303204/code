        <maml:description>
          <maml:para>Specifica il progetto da analizzare e a cui aggiungere reindirizzamenti di binding.</maml:para>
        </maml:description>
        <command:parameterValue required="true">
          string
        </command:parameterValue>
      </command:parameter>
    </command:parameters>

    <command:examples>
      <command:example>
        <maml:title>----------  ESEMPIO 1  ----------</maml:title>
        <dev:code>PM&gt; Add-BindingRedirect MyProjectName</dev:code>
