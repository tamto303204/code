        <maml:title>----------  ESEMPIO 1  ----------</maml:title>
        <dev:code>PM&gt; Get-Project</dev:code>
        <dev:remarks>
          <maml:para>
            Descrizione ------------------------------------------------------------------ Restituisce un riferimento al progetto predefinito.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>----------  ESEMPIO 2  ----------</maml:title>
        <dev:code>PM&gt; Get-Project MyProjectName</dev:code>
        <dev:remarks>
          <maml:para>
            Descrizione ------------------------------------------------------------------ Restituisce un riferimento al progetto specificato MyProjectName.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>----------  ESEMPIO 3  ----------</maml:title>
        <dev:code>PM&gt; Get-Project -All</dev:code>
