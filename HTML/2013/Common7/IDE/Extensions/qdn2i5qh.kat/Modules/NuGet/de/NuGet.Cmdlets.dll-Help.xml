    <maml:description>
      <maml:para>
        Untersucht alle Assemblys im Ausgabepfad auf ein Projekt und fügt der Anwendungs- oder Webkonfigurationsdatei Bindungsweiterleitungen hinzu, wenn erforderlich. HINWEIS: Ab NuGet 1.2 führt NuGet diesen Befehl beim Installieren eines Pakets automatisch aus.
      </maml:para>
    </maml:description>

    <!-- Cmdlet syntax section-->
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Add-BindingRedirect</maml:name>
        <command:parameter required="true" position="1">
          <maml:name>ProjectName</maml:name>
          <command:parameterValue required="true">string</command:parameterValue>
        </command:parameter>
      </command:syntaxItem>
    </command:syntax>

    <!-- Cmdlet parameter section  -->
    <command:parameters>
      <command:parameter required="true" position="1">
        <maml:name>ProjectName</maml:name>
        <maml:description>
          <maml:para>Gibt das Projekt an, das analysiert werden soll und dem Bindungsweiterleitungen hinzugefügt werden sollen.</maml:para>
        </maml:description>
        <command:parameterValue required="true">
          string
        </command:parameterValue>
      </command:parameter>
    </command:parameters>

    <command:examples>
      <command:example>
        <maml:title>----------  BEISPIEL 1  ----------</maml:title>
        <dev:code>PM&gt; Add-BindingRedirect MyProjectName</dev:code>
        <dev:remarks>
          <maml:para>
            Beschreibung ------------------------------------------------------------------ Fügt dem angegebenen Projekt ("MyProjectName") Bindungsweiterleitungen hinzu.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
    </command:examples>

