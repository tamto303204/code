  <!-- Open-PackagePage -->
  <command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10">

    <command:details>
      <command:name>
        Open-PackagePage
      </command:name>
      <maml:description>
        <maml:para>
          Öffnet den Browser mit den URLs "ProjectUrl", "LicenseUrl" oder "ReportAbuseUrl" des angegebenen Pakets.
        </maml:para>
      </maml:description>
      <command:verb>Open</command:verb>
      <command:noun>PackagePage</command:noun>
      <dev:version></dev:version>
    </command:details>

    <maml:description>
      <maml:para>
        Öffnet den Browser mit den URLs "ProjectUrl", "LicenseUrl" oder "ReportAbuseUrl" des angegebenen Pakets.
      </maml:para>
    </maml:description>

    <!-- Cmdlet syntax section-->
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Open-PackagePage</maml:name>
        <command:parameter required="true" position="named">
          <maml:name>Id</maml:name>
          <command:parameterValue required="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>Version</maml:name>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>Source</maml:name>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>License</maml:name>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>ReportAbuse</maml:name>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>PassThru</maml:name>
        </command:parameter>
      </command:syntaxItem>
    </command:syntax>

    <!-- Cmdlet parameter section  -->
    <command:parameters>
      <command:parameter required="false" position="named">
        <maml:name>Id</maml:name>
        <maml:description>
          <maml:para>Gibt die ID des Pakets an, nach dem gesucht werden soll.</maml:para>
        </maml:description>
        <command:parameterValue required="true">string</command:parameterValue>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>Version</maml:name>
        <maml:description>
          <maml:para>Gibt die Version des Pakets an, nach dem gesucht werden soll. Wenn dieser Parameter nicht angegeben wird, wird standardmäßig die aktuellste Version verwendet.</maml:para>
        </maml:description>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>Source</maml:name>
        <maml:description>
          <maml:para>Gibt die Quelle des Repositorys an, in dem nach dem Paket gesucht werden soll. Wenn dieser Parameter nicht angegeben wird, wird standardmäßig die ausgewählte Quelle im Paketquellen-Dropdownsteuerelement verwendet.</maml:para>
        </maml:description>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>License</maml:name>
        <maml:description>
          <maml:para>Gibt an, dass das Cmdlet die "LicenseUrl" des angegebenen Pakets öffnen soll. Wenn weder die "LicenseUrl" noch die "ReportAbuseUrl" festgelegt ist, öffnet das Cmdlet standardmäßig die "ProjectUrl".</maml:para>
        </maml:description>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>ReportAbuse</maml:name>
        <maml:description>
          <maml:para>Gibt an, dass das Cmdlet die "ReportAbuseUrl" des angegebenen Pakets öffnen soll. Wenn weder die "LicenseUrl" noch die "ReportAbuseUrl" festgelegt ist, öffnet das Cmdlet standardmäßig die "ProjectUrl".</maml:para>
        </maml:description>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>PassThru</maml:name>
        <maml:description>
          <maml:para>Wenn dieser Parameter angegeben wird, gibt das Cmdlet den Wert der angeforderten URL zurück</maml:para>
        </maml:description>
      </command:parameter>
    </command:parameters>

    <command:examples>
      <command:example>
        <maml:title>----------  BEISPIEL 1  ----------</maml:title>
        <dev:code>PM&gt; Open-PackagePage Ninject</dev:code>
        <dev:remarks>
          <maml:para>
            Beschreibung ------------------------------------------------------------------ Öffnet einen Browser mit der im Ninject-Paket angegebenen Projekt-URL.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>----------  BEISPIEL 2  ----------</maml:title>
        <dev:code>PM&gt; Open-PackagePage Ninject -License</dev:code>
        <dev:remarks>
          <maml:para>
            Beschreibung ------------------------------------------------------------------ Öffnet einen Browser mit der im Ninject-Paket angegebenen Lizenz-URL.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>----------  BEISPIEL 3  ----------</maml:title>
        <dev:code>PM&gt; Open-PackagePage Ninject -ReportAbuse</dev:code>
        <dev:remarks>
          <maml:para>
            Beschreibung ------------------------------------------------------------------ Öffnet einen Browser mit der URL in der aktuellen Paketquelle, die zum Angeben des Missbrauchs des angegebenen Pakets verwendet wird.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>----------  BEISPIEL 4  ----------</maml:title>
        <dev:code>PM&gt; $url = Open-PackagePage Ninject -License -WhatIf -PassThru</dev:code>
        <dev:remarks>
          <maml:para>
            Beschreibung ------------------------------------------------------------------ Weist der Variablen ("$url") eine Lizenz-URL zu, ohne die URL in einem Browser zu öffnen.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
    </command:examples>

    <!-- Link section  -->
    <maml:relatedLinks>
      <maml:navigationLink>
        <maml:linkText>Onlineversion:</maml:linkText>
        <maml:uri>http://docs.nuget.org/</maml:uri>
      </maml:navigationLink>
      <maml:navigationLink>
        <maml:linkText>Open-PackagePage</maml:linkText>
        <maml:uri/>
      </maml:navigationLink>
    </maml:relatedLinks>
  </command:command>
