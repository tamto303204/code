  <!-- Get-Project -->
  <command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10">

    <command:details>
      <command:name>
        Get-Project
      </command:name>
      <maml:description>
        <maml:para>Ruft das angegebene Projekt ab. Wenn keine Angabe erfolgt, wird das Standardprojekt zurückgegeben.</maml:para>
      </maml:description>
      <command:verb>Get</command:verb>
      <command:noun>Project</command:noun>
      <dev:version></dev:version>
    </command:details>

    <maml:description>
      <maml:para>Gibt einen Verweis auf die DTE (Development Tools Environment) für das angegebene Projekt zurück. Wenn keine Angabe erfolgt, wird das Standardprojekt zurückgegeben, das in der Konsole des Paket-Managers ausgewählt ist.</maml:para>
    </maml:description>

    <!-- Cmdlet syntax section-->
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Get-Project</maml:name>
        <command:parameter required="false" position="1">
          <maml:name>Name</maml:name>
          <command:parameterValue required="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>All</maml:name>
        </command:parameter>
      </command:syntaxItem>
    </command:syntax>

    <!-- Cmdlet parameter section  -->
    <command:parameters>
      <command:parameter required="false" position="1">
        <maml:name>Name</maml:name>
        <maml:description>
          <maml:para>Gibt das zurückzugebende Projekt an. Wenn dieser Parameter nicht angegeben wird, wird das Standardprojekt zurückgegeben, das in der Konsole des Paket-Managers ausgewählt ist.</maml:para>
        </maml:description>
        <command:parameterValue required="true">
          string
        </command:parameterValue>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>All</maml:name>
        <maml:description>
          <maml:para>Gibt jedes Paket in der Lösung zurück.</maml:para>
        </maml:description>
      </command:parameter>
    </command:parameters>

    <command:examples>
      <command:example>
        <maml:title>----------  BEISPIEL 1  ----------</maml:title>
        <dev:code>PM&gt; Get-Project</dev:code>
        <dev:remarks>
          <maml:para>
            Beschreibung ------------------------------------------------------------------ Gibt einen Verweis auf das Standardprojekt zurück.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>----------  BEISPIEL 2  ----------</maml:title>
        <dev:code>PM&gt; Get-Project MyProjectName</dev:code>
        <dev:remarks>
          <maml:para>
            Beschreibung ------------------------------------------------------------------ Gibt einen Verweis auf das angegebene Projekt ("MyProjectName") zurück.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>----------  BEISPIEL 3  ----------</maml:title>
        <dev:code>PM&gt; Get-Project -All</dev:code>
        <dev:remarks>
          <maml:para>
            Beschreibung ------------------------------------------------------------------ Gibt einen Verweis auf jedes Projekt in der Lösung zurück.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
    </command:examples>

