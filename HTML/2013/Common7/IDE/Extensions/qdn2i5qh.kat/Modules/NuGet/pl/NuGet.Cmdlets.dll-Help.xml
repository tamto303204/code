<helpItems schema="maml">

  <!-- Install-Package command -->
  <command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10">

    <command:details>
      <command:name>
        Install-Package
      </command:name>
      <maml:description>
        <maml:para>Instaluje pakiet.</maml:para>
      </maml:description>
      <command:verb>Install</command:verb>
      <command:noun>Package</command:noun>
      <dev:version></dev:version>
    </command:details>

    <maml:description>
      <maml:para>Instaluje pakiet i jego zależności w projekcie.</maml:para>
    </maml:description>

    <!-- Cmdlet syntax section-->
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Install-Package</maml:name>
        <command:parameter required="true" position="1">
          <maml:name>Id</maml:name>
          <command:parameterValue required="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>IgnoreDependencies</maml:name>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>ProjectName</maml:name>
          <command:parameterValue required="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" position="2">
          <maml:name>Version</maml:name>
          <command:parameterValue required="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" position="3">
          <maml:name>Source</maml:name>
          <command:parameterValue required="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>IncludePrerelease</maml:name>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>FileConflictAction</maml:name>
        </command:parameter>
      </command:syntaxItem>
    </command:syntax>

    <!-- Cmdlet parameter section  -->
    <command:parameters>
      <command:parameter required="true" position="1">
        <maml:name>Id</maml:name>
        <maml:description>
          <maml:para>Określa identyfikator pakietu do zainstalowania.</maml:para>
        </maml:description>
        <command:parameterValue required="true">
          string
        </command:parameterValue>
        <dev:defaultValue></dev:defaultValue>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>IgnoreDependencies</maml:name>
        <maml:description>
          <maml:para>Instaluje tylko ten pakiet bez jego zależności.</maml:para>
        </maml:description>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>ProjectName</maml:name>
        <maml:description>
          <maml:para>Określa projekt, w którym ma zostać zainstalowany pakiet. W przypadku jego pominięcia jest wybierany projekt domyślny.</maml:para>
        </maml:description>
        <command:parameterValue required="true">
          string
        </command:parameterValue>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>Source</maml:name>
        <maml:description>
          <maml:para>Określa adres URL lub ścieżkę katalogu źródła pakietów zawierającego pakiet do zainstalowania. W przypadku ustawienia ścieżki lokalnego systemu plików element Source może być elementem bezwzględnym lub względnym w stosunku do bieżącego katalogu. W przypadku jego pominięcia odpowiedni adres URL pakietu jest wyszukiwany w obecnie wybranym źródle pakietów.</maml:para>
        </maml:description>
        <command:parameterValue required="true">
          string
        </command:parameterValue>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>Version</maml:name>
        <maml:description>
          <maml:para>Określa wersję pakietu do zainstalowania. W przypadku jej pominięcia domyślna jest najnowsza wersja.</maml:para>
        </maml:description>
        <command:parameterValue required="true">
          string
        </command:parameterValue>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>IncludePrerelease</maml:name>
        <maml:description>
          <maml:para>Określa, czy to polecenie będzie uwzględniać pakiety w wersji wstępnej. W przypadku pominięcia tego ustawienia są uwzględniane tylko pakiety w wersji stabilnej.</maml:para>
        </maml:description>
      </command:parameter>
        <command:parameter required="false" position="named">
            <maml:name>FileConflictAction</maml:name>
            <maml:description>
                <maml:para>Wskazuje, co te polecenie powinno zrobić, jeśli w projekcie występuje już plik z folderu zawartości pakietu.</maml:para>
            </maml:description>
        </command:parameter>
    </command:parameters>

    <!-- Examples section  -->
    <command:examples>
      <command:example>
        <maml:title>---------- PRZYKŁAD 1 ----------</maml:title>
        <dev:code>PM&gt; Install-Package Elmah</dev:code>
        <dev:remarks>
          <maml:para>
            Opis ------------------------------------------------------------------ Instaluje najnowszą wersję pakietu Elmah z bieżącego źródła pakietów w projekcie domyślnym.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>---------- PRZYKŁAD 2 ----------</maml:title>
        <dev:code>PM&gt; Install-Package Glimpse -Version 1.0.0 -Project MvcApplication1 </dev:code>
        <dev:remarks>
          <maml:para>
            Opis ------------------------------------------------------------------ Instaluje pakiet Glimpse w wersji 1.0.0 w projekcie MvcApplication1.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>---------- PRZYKŁAD 3 ----------</maml:title>
        <dev:code>PM&gt; Install-Package Ninject.Mvc3 -IgnoreDependencies -Source c:\temp\packages </dev:code>
        <dev:remarks>
          <maml:para>
            Opis ------------------------------------------------------------------ Instaluje pakiet Ninject.Mvc3 bez jego zależności. Wyszukuje pakiet w katalogu c:\temp\packages.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
    </command:examples>

