  <!-- Open-PackagePage -->
  <command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10">

    <command:details>
      <command:name>
        Open-PackagePage
      </command:name>
      <maml:description>
        <maml:para>
          Otwórz przeglądarkę z adresem ProjectUrl, LicenseUrl lub ReportAbuseUrl określonego pakietu.
        </maml:para>
      </maml:description>
      <command:verb>Open</command:verb>
      <command:noun>PackagePage</command:noun>
      <dev:version></dev:version>
    </command:details>

    <maml:description>
      <maml:para>
        Otwórz przeglądarkę z adresem ProjectUrl, LicenseUrl lub ReportAbuseUrl określonego pakietu.
      </maml:para>
    </maml:description>

    <!-- Cmdlet syntax section-->
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Open-PackagePage</maml:name>
        <command:parameter required="true" position="named">
          <maml:name>Id</maml:name>
          <command:parameterValue required="true">string</command:parameterValue>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>Version</maml:name>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>Source</maml:name>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>License</maml:name>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>ReportAbuse</maml:name>
        </command:parameter>
        <command:parameter required="false" position="named">
          <maml:name>PassThru</maml:name>
        </command:parameter>
      </command:syntaxItem>
    </command:syntax>

    <!-- Cmdlet parameter section  -->
    <command:parameters>
      <command:parameter required="false" position="named">
        <maml:name>Id</maml:name>
        <maml:description>
          <maml:para>Określa identyfikator pakietu do wyszukania.</maml:para>
        </maml:description>
        <command:parameterValue required="true">string</command:parameterValue>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>Version</maml:name>
        <maml:description>
          <maml:para>Określa wersję pakietu do wyszukania. W przypadku jej pominięcia domyślna jest najnowsza wersja.</maml:para>
        </maml:description>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>Source</maml:name>
        <maml:description>
          <maml:para>Określa źródło repozytorium, w którym ma zostać wyszukany pakiet. W przypadku jego pominięcia domyślnie jest ustawiane źródło wybrane za pomocą kontrolki rozwijanej źródła pakietów.</maml:para>
        </maml:description>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>License</maml:name>
        <maml:description>
          <maml:para>Określa, czy polecenie cmdlet ma otwierać adres LicenseUrl danego pakietu. Jeśli nie ustawiono adresu LicenseUrl ani ReportAbuseUrl, polecenie cmdlet domyślnie otwiera adres ProjectUrl.</maml:para>
        </maml:description>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>ReportAbuse</maml:name>
        <maml:description>
          <maml:para>Określa, czy polecenie cmdlet ma otwierać adres ReportAbuseUrl danego pakietu. Jeśli nie ustawiono adresu LicenseUrl ani ReportAbuseUrl, polecenie cmdlet domyślnie otwiera adres ProjectUrl.</maml:para>
        </maml:description>
      </command:parameter>
      <command:parameter required="false" position="named">
        <maml:name>PassThru</maml:name>
        <maml:description>
          <maml:para>W przypadku określenia tego ustawienia polecenie cmdlet zwraca wartość żądanego adresu URL.</maml:para>
        </maml:description>
      </command:parameter>
    </command:parameters>

    <command:examples>
      <command:example>
        <maml:title>---------- PRZYKŁAD 1 ----------</maml:title>
        <dev:code>PM&gt; Open-PackagePage Ninject</dev:code>
        <dev:remarks>
          <maml:para>
            Opis ------------------------------------------------------------------ Otwiera w przeglądarce adres URL projektu określony w pakiecie Ninject.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>---------- PRZYKŁAD 2 ----------</maml:title>
        <dev:code>PM&gt; Open-PackagePage Ninject -License</dev:code>
        <dev:remarks>
          <maml:para>
            Opis ------------------------------------------------------------------ Otwiera w przeglądarce adres URL licencji określony w pakiecie Ninject.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>---------- PRZYKŁAD 3 ----------</maml:title>
        <dev:code>PM&gt; Open-PackagePage Ninject -ReportAbuse</dev:code>
        <dev:remarks>
          <maml:para>
            Opis ------------------------------------------------------------------ Otwiera w przeglądarce adres URL bieżącego źródła pakietów używany do zgłoszenia nadużycia związanego z określonym pakietem.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>---------- PRZYKŁAD 4 ----------</maml:title>
        <dev:code>PM&gt; $url = Open-PackagePage Ninject -License -WhatIf -PassThru</dev:code>
        <dev:remarks>
          <maml:para>
            Opis ------------------------------------------------------------------ Przypisuje adres URL do zmiennej $url bez otwierania adresu URL w przeglądarce.
          </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
      </command:example>
    </command:examples>

